<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-10 text-white">
    <div class="cards flex flex-wrap gap-10">
        <% user.posts.forEach(function(ele){ %>
            <div class="card w-32">
                <div class="card w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
                    <img src="/images/uploads/<%= ele.image %>" class="w-full h-full object-cover" alt="">
                </div>
                <h5 class="mt-2 text-sm"><%= ele.title %></h5>
            </div>
        <% }) %>
    </div>
</div>
<% include ./partials/footer.ejs %>
